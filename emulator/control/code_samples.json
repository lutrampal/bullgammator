		"series3": "-- // Description--%n%-- //      Algorithme qui approche les solutions d'une equation du second--%n%-- //      degre.--%n%-- //--%n%-- // Resultat--%n%-- //      Stock la ou les racines dans x1 et x2. Si le determinant est negatif,--%n%-- //      on met les solutions à 0. On sait que c'est une erreur si b ou c--%n%-- //      sont non nuls.--%n%-- //--%n%-- // Historique--%n%-- //      Crée le 28/01/18--%n%-- --%n%-- {--%n%--     float x1;--%n%--     float x2;--%n%--     float a = -1.2646485231;--%n%--     float b = -2.8955232;--%n%--     float c = 5.4584531;--%n%-- --%n%--     // Calcul de la racine--%n%--     float determinantAuCarre = b*b - 4.0*a*c;--%n%--     float rk = 0.0;--%n%--     float rkSuiv = 2.0;--%n%-- --%n%--     if(determinantAuCarre < 0.0) {--%n%--         x1 = 0.0;--%n%--         x2 = x1;--%n%--     } else if (determinantAuCarre == 0.0) {--%n%--         x1 = -b/(2.0*a);--%n%--         x2 = x1;--%n%--     } else {--%n%--         rk = 1.0;--%n%--         while(rk != rkSuiv) {--%n%--             rk = rkSuiv;--%n%--             rkSuiv = (rk + determinantAuCarre/rk)/2.0;--%n%--         }--%n%--         // Calcul des solutions--%n%--         x1 = (-b + rk)/(2.0*a);--%n%--         x2 = (-b - rk)/(2.0*a);--%n%--     }--%n%--     println(x1);--%n%--     println(x2);--%n%-- }--%n%--%n%2001 2203 1001--%t%1003--%n%--%n%",
